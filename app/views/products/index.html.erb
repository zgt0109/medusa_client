<div class="row">
<div class="col-sm-9 col-md-6">
    <div class="page-header">
    <h1><%=t '.title', :default => "软件名称列表" %></h1>
    </div>
    <table class="table table-striped">
    <thead>
        <tr>
        <th width="40%">软件名</th>
        <th width="50%" class="text-right">操作</th>
        </tr>
    </thead>
    <tbody id="productsTableTbody">
        <%= render @products  %>
    </tbody>
    </table>

    <div class="bs-component">
        <%= will_paginate @products, renderer: BootstrapPagination::Rails %>
    </div>

    <%= link_to new_product_path, class: "btn btn-primary f-s-12" do %>
    <i class="fa fa-plus m-r-xs"></i>添加
    <% end %>
</div>
</div>